package com.rs.core.java.practice;

public class FibonacciArray {
    public static void main(String[] args) {
        int n = 10; // Change the value to the desired number of terms

        System.out.print("Fibonacci Series (Array): ");
        int[] fibonacciArray = new int[n];
        fibonacciArray[0] = 0;
        fibonacciArray[1] = 1;

        for (int i = 2; i < n; i++) {
            fibonacciArray[i] = fibonacciArray[i - 1] + fibonacciArray[i - 2];
            System.out.print(fibonacciArray[i]+" "+fibonacciArray[i]+" "+fibonacciArray[i] + " ");

        }

        for (int i = 0; i < n; i++) {
            System.out.print(fibonacciArray[i] + " ");
        }
    }
}
